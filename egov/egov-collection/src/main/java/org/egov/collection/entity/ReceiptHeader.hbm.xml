	<query name="RECEIPTS_FOR_VOUCHERS">
	<![CDATA[select receiptHeader from org.egov.collection.entity.ReceiptVoucher rv where rv.voucherheader.id in (:param_0)]]></query>

	<query name="RECEIPTS_FOR_BOUNCED_INSTRUMENTS">
	<![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh join rh.receiptVoucher as rv where rh.status.code<>? and rv.voucherheader.id in (:param_1)]]></query>

    <query name="RECEIPTS_BY_RECEIPTNUM">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh where rh.receiptnumber=?]]></query>

	<query name="RECEIPTS_BY_RECEIPTNUM_AND_SERVICECODE">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh where rh.service=? and (rh.receiptnumber=? or rh.manualreceiptnumber=?)]]></query>

	<query name="RECEIPTS_BY_REFNUM_AND_SERVICECODE">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh where rh.referencenumber=? and rh.service=?]]></query>

	<query name="RECEIPTS_BY_INSTRUMENTNO_AND_SERVICECODE">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh left join rh.receiptInstrument as ri where ri.instrumentNumber=?  and rh.service=?]]></query>

   	<query name="RECEIPTS_BY_RECONSTATUS">
    <![CDATA[select receipt from org.egov.collection.entity.ReceiptHeader receipt where receipt.isReconciled = ?]]></query>

   <!--  <query name="CREATEDBYUSERS_OF_RECEIPTS">
    <![CDATA[select distinct createdBy from org.egov.collection.entity.ReceiptHeader rh where rh.createdBy.type not in ('CITIZEN','SYSTEM')]]></query> -->

    <query name="RECEIPT_BY_CHALLANID">
    <![CDATA[select receipt from org.egov.collection.entity.ReceiptHeader receipt, org.egov.collection.entity.Challan cl WHERE cl.receiptHeader.id = receipt.id and cl.id = ?]]></query>

    <query name="VALIDRECEIPT_BY_CHALLANNO">
    <![CDATA[select receipt from org.egov.collection.entity.ReceiptHeader as receipt left join receipt.challan as challan where challan.challanNumber=? and challan.state.value='END' and challan.challanDate <= current_date and challan.validUpto >= current_date]]></query>

    <query name="CHALLANRECEIPT_BY_REFERENCEID">
    <![CDATA[select receipt2 from org.egov.collection.entity.ReceiptHeader as receipt1,org.egov.collection.entity.ReceiptHeader as receipt2 left join receipt2.challan as challan where receipt2.receiptHeader.id=receipt1.id and receipt1.id = ?]]></query>

    <query name="QUERY_ONLINE_PENDING_RECEIPTS_BY_CONSUMERCODE_AND_SERVICECODE">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh, org.egov.collection.entity.OnlinePayment online where rh.id = online.receiptHeader.id and rh.service=? and rh.consumerCode=? and online.status.code = ?]]></query>

    <query name="RECEIPT_BY_SERVICE_RECEIPTNUMBER_CONSUMERCODE">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh where (rh.receiptnumber=? or rh.manualreceiptnumber=?) and rh.consumerCode=?]]></query>

    <query name="RECEIPT_BY_RECEIPTID_AND_REFERENCENUMBER">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh, org.egov.collection.entity.OnlinePayment online where rh.id = online.receiptHeader.id and rh.id=? and rh.referencenumber = ? ]]></query>

    <query name="RECEIPT_BY_SERVICE_MANUALRECEIPTNO_AND_DATE">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh where rh.manualreceiptnumber = ? and rh.service = ? and rh.manualreceiptdate between ? and ? and rh.status.code<>? ]]></query>

    <query name="QUERY_RECEIPT_BY_ID_AND_CONSUMERCODE">
	<![CDATA[
	select rh from org.egov.collection.entity.ReceiptHeader rh where rh.id = ? and replace(replace(rh.consumerCode,'-',''),'/','') = ?
	]]>
	</query>

	<query name="QUERY_RECEIPT_BY_ID_AND_CITYCODE">
	<![CDATA[
	select rh from org.egov.collection.entity.ReceiptHeader rh, org.egov.infra.admin.master.entity.City city where rh.id = ? and city.code = ?
	]]>
	</query>

	<query name="RECEIPTS_BY_DATE_AND_SERVICECODE">
	<![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh where date(rh.receiptdate) between ? and ? and rh.service = ? and  rh.source=? and rh.status.code in ('TO_BE_SUBMITTED','SUBMITTED','APPROVED','REMITTED') ]]>
	</query>

	<query name="RECEIPTS_BY_INSTRUMENTHEADER_AND_SERVICECODE">
    <![CDATA[select rh from org.egov.collection.entity.ReceiptHeader rh left join rh.receiptInstrument as ri where  rh.service=? and ri.id in (:param_1)  ]]>
    </query>

	<query name="STATUS_OF_RECEIPTS">
    <![CDATA[select distinct rh.status from org.egov.collection.entity.ReceiptHeader rh where rh.status.code<>'CANCELLED']]>
    </query>

	<query name="RECEIPT_BY_ID_AND_STATUSNOTCANCELLED">
	<![CDATA[
	select rh from org.egov.collection.entity.ReceiptHeader rh where rh.id = ? and rh.status.code<>'CANCELLED']]>
	</query>

  	<query name="DISTINCT_SERVICE_DETAILS">
    <![CDATA[select distinct rh.service from org.egov.collection.entity.ReceiptHeader rh where rh.status.code<>'CANCELLED']]>
    </query>

    <query name="QUERY_PENDING_RECEIPT_BY_ID_AND_CONSUMERCODE">
	<![CDATA[
	select rh from org.egov.collection.entity.ReceiptHeader rh where rh.status.code ='PENDING' and rh.id = ? and replace(replace(rh.consumerCode,'-',''),'/','') = ?
	]]>
	</query>

	<query name="QUERY_PENDING_RECEIPT_BY_ID_AND_CITYCODE">
	<![CDATA[
	select rh from org.egov.collection.entity.ReceiptHeader rh, org.egov.infra.admin.master.entity.City city where rh.status.code ='PENDING' and rh.id = ? and city.code = ?
	]]>
	</query>
