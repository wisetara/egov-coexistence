	<queryString>
		<![CDATA[SELECT
CH.RECEIPTNUMBER AS RECEIPT_NUMBER ,CH.TOTALAMOUNT as RECEIPT_AMOUNT,
INS.INSTRUMENTNUMBER as CHEQUE_DD_NO,
INS.INSTRUMENTDATE  as CHEQUE_DD_DATE, BANK.NAME AS DEPOSITEDINBANK,
INS.INSTRUMENTAMOUNT AS DISHONERED_AMOUNT,STA.DESCRIPTION AS STATUS,
CH.ID as selectedReceipts, STA1.DESCRIPTION AS RECEIPT_STATUS
FROM
EGCL_COLLECTIONHEADER CH INNER JOIN  egcl_collectioninstrument CINS ON CH.ID = CINS.COLLECTIONHEADER
INNER JOIN  EGF_INSTRUMENTHEADER INS ON CINS.INSTRUMENTHEADER = INS.ID
INNER JOIN  bankaccount BACC ON INS.BANKACCOUNTID = BACC.ID
INNER JOIN  bankbranch BRANCH ON BACC.BRANCHID = BRANCH.ID
INNER JOIN  BANK BANK ON BRANCH.BANKID = BANK.ID
INNER JOIN  EGCL_SERVICEDETAILS SER ON SER.ID = CH.SERVICEDETAILS
INNER JOIN  EGW_STATUS STA ON INS.ID_STATUS=STA.ID
INNER JOIN  EGW_STATUS STA1 ON CH.STATUS=STA1.ID
INNER JOIN  EGF_INSTRUMENTTYPE INSTP ON  INSTP.ID = INS.INSTRUMENTTYPE
INNER JOIN EG_LOCATION EG_LOCATION ON CH.LOCATION=EG_LOCATION.ID
FULL OUTER JOIN EG_LOCATION MAINLOCATION ON EG_LOCATION.ID=MAINLOCATION.ID

WHERE
STA.MODULETYPE = 'Instrument'
AND STA.DESCRIPTION IN ('Dishonored','dishonour cheque in workflow')
$P!{Q_FROMDATE}
$P!{Q_TODATE}
$P!{Q_SERVICE_ID}
$P!{Q_PAYMENT_MODES}
$P!{Q_STATUS_ID}
$P!{Q_LOCATION_ID}
$P!{Q_INSTRUMENT_NUMBER}
ORDER BY INS.LASTMODIFIEDDATE]]>
	</queryString>
